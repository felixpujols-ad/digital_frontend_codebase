{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@badeball/cypress-cucumber-preprocessor/*": [
        "./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"
      ]
    },
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "module": "Node16",
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node",
      "cypress-axe",
      "cypress-real-events"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}